{
  "name": "jsparsec",
  "version": "1.0.0",
  "description": "NPM package boilerplate with tests and babel for ES2015",
  "main": "./lib/main",
  "scripts": {
    "test": "chmod +x test.sh; ./test.sh",
    "gendoc": "./node_modules/jsdox/bin/jsdox --output docs src",
    "build": "./node_modules/babel-cli/bin/babel.js src --presets babel-preset-es2015 --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgustinCB/jsparsec.git"
  },
  "keywords": [
    "nomads",
    "parser",
    "combinator"
  ],
  "author": "AgustinCB",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/AgustinCB/jsparsec/issues"
  },
  "homepage": "https://github.com/AgustinCB/jsparsec#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "jsdox": "^0.4.10",
    "mocha": "^2.5.3",
    "standard": "^7.1.2"
  }
}
